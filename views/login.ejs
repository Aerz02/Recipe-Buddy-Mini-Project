<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet"  type="text/css" href="css/style.css" />
    <title>Login to <%= appName %> </title>
</head>
<body>
    <% if (!loggedin) { %>
        <%- include("partials/navbar.ejs") %>
    <% } else { %>
        <%- include("partials/navbar loggedin.ejs") %>
    <% } %> 
    <h1>Login to <%=appName%></h1>
    <form method="POST" action="/loggedin">
        <p>Username: <input id="username" type="text" name="username" placeholder="username"/></p>
        <p>Password: <input id="password" type="password" name="password" placeholder="password"/></p>
        <input type="submit" value="Login">
    </form>
     
</body>
</html>